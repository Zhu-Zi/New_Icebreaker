@{
    Layout = null;
}
@using Icebreaker.Controllers;
@using System.Data;
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width" />

    <link rel="icon" href="@Url.Content("../../favicon.ico")"/>

    <title>成绩查询结果</title>

    <!-- Bootstrap core CSS -->
    <link href="@Url.Content("~/Content/StyleSheet/bootstrap.min.css")" rel="stylesheet"/>

    <link href="@Url.Content("~/Content/StyleSheet/bootstrap-theme.min.css")" rel="stylesheet"/>

    <!-- Custom styles for this template -->
    <link href="@Url.Content("~/Content/StyleSheet/mainStyleSheet.css")" rel="stylesheet"/>

    
        
        <script type="text/javascript" src="@Url.Content("~/Content/Javascript/jquery-1.11.3.min.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Content/Javascript/bootstrap.js")"></script>
    <script>
        var back = function () {
            history.go(-1);
        }
    </script>  
</head>
<body>
    <h1 class="text-center">萌萌哒地查成绩－结果</h1>
    <div class="container">
        <button onclick="back()" class="btn btn-info">返回</button>
        <table class="table" id="tbList">
            <thead id=" tableHead">
                <tr>
                    <th nowrap>选课课号</th>
                    <th nowrap>课程名称</th>
                    <th nowrap>教师姓名</th>
                    <th nowrap>学分</th>
                    <th nowrap>周学时</th>
                    <th nowrap>上课地点</th>
                    <th nowrap>上课时间</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var a in ViewData["ChoseClass"] as List<Icebreaker.ViewModels.ViewChoseCourseTable>)
                {
                    <tr>
                        <td>@a.CourseNumber</td>
                        <td>@a.CourseName</td>
                        <td>@a.Teacher</td>
                        <td>@a.Credit</td>
                        <td>@a.WeekPeriod</td>
                        <td>@a.ClassRoom</td>
                        <td>@a.SchoolTime</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

</body>
</html>
